{
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "commit": "git-cz",
        "prettier": "prettier --config .prettierrc.json --ignore-path .prettierignore ./resources/js",
        "prettier:check": "npm run prettier -- --check",
        "prettier:fix": "npm run prettier -- --write",
        "lint": "eslint --ext .tsx resources/js",
        "lint:fix": "eslint --fix --ext .tsx resources/js",
        "test": "jest"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "dependencies": {
        "laravel-echo": "^1.14.0",
        "pusher-js": "^7.4.0",
        "react-app-polyfill": "^3.0.0"
    },
    "peerDependencies": {
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@babel/plugin-syntax-flow": "^7.18.6",
        "@babel/plugin-transform-react-jsx": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@commitlint/cz-commitlint": "^17.0.3",
        "@headlessui/react": "^1.4.2",
        "@inertiajs/inertia": "^0.11.0",
        "@inertiajs/inertia-react": "^0.8.0",
        "@inertiajs/progress": "^0.2.6",
        "@tailwindcss/forms": "^0.5.2",
        "@testing-library/dom": "^8.17.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^12.1.5",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^28.1.7",
        "@types/node": "^18.7.6",
        "@types/react-dom": "^18.0.6",
        "@types/styled-components": "^5.1.26",
        "@types/testing-library__jest-dom": "^5.14.5",
        "@typescript-eslint/eslint-plugin": "^5.33.1",
        "@typescript-eslint/parser": "^5.33.1",
        "@vitejs/plugin-react": "^2.0.0",
        "autoprefixer": "^10.4.2",
        "axios": "^0.27",
        "babel-jest": "^28.1.3",
        "commitizen": "^4.0.3",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react-app": "^7.0.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest-dom": "^4.0.2",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-sonarjs": "^0.15.0",
        "eslint-plugin-testing-library": "^5.6.0",
        "git-cz": "^4.9.0",
        "glob": "^8.0.3",
        "husky": "^8.0.1",
        "inquirer": "^8.0.0",
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^28.1.3",
        "laravel-vite-plugin": "^0.5.0",
        "lodash": "^4.17.19",
        "postcss": "^8.4.6",
        "prettier": "^2.7.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "tailwind-styled-components": "^2.1.8",
        "tailwindcss": "^3.1.0",
        "ts-jest": "^28.0.8",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4",
        "vite": "^3.0.0",
        "webpack": "^5.74.0"
    },
    "jest": {
        "collectCoverage": true,
        "collectCoverageFrom": [
            "resources/js/**/*.tsx",
            "resources/js/Components/*.tsx",
            "!**/node_modules/**",
            "!resources/js/Pages/**",
            "!resources/js/Layouts/**",
            "!resources/js/Interfaces/**",
            "!resources/js/app.tsx"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 85,
                "branches": 50,
                "functions": 85,
                "lines": 85
            }
        },
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/resources/js/setupTests.ts"
        ],
        "testEnvironment": "jsdom"
    }
}
